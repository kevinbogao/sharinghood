{
  "name": "sharinghood-client",
  "description": "Web client for Sharinghood",
  "version": "0.1.0",
  "repository": "https://github.com/kevinbogao/sharinghood/tree/master/client",
  "contributors": [
    "Sparsh Tyagi <sparsh.tyagi@code.berlin>",
    "Bo Gao <bo.gao@code.berlin>"
  ],
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks"
    ],
    "plugins": [
      "graphql"
    ],
    "rules": {
      "indent": "off",
      "no-confusing-arrow": "off",
      "no-console": "off",
      "no-shadow": "off",
      "no-nested-ternary": "off",
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id",
            "_doc"
          ]
        }
      ],
      "no-plusplus": "off",
      "no-return-assign": "off",
      "nonblock-statement-body-position": "off",
      "no-extra-boolean-cast": "off",
      "curly": "off",
      "one-var": "off",
      "function-paren-newline": "off",
      "implicit-arrow-linebreak": "off",
      "one-var-declaration-per-line": "off",
      "object-curly-newline": "off",
      "operator-linebreak": "off",
      "jsx-a11y/label-has-associated-control": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "babel": {
    "plugins": [
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "styled-jsx-plugin-sass"
          ]
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.1",
    "@babel/runtime": "^7.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.6",
    "apollo-link-token-refresh": "^0.3.2",
    "dotenv": "^8.2.0",
    "firebase": "^7.21.1",
    "graphql": "^15.3.0",
    "jspdf": "^2.1.1",
    "jwt-decode": "^3.0.0",
    "moment": "^2.29.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dates": "^21.8.0",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.11",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-with-direction": "^1.3.1",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "4.1.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "styled-jsx": "^3.3.0",
    "styled-jsx-plugin-sass": "^1.0.0"
  }
}
