{
  "name": "sharinghood-client",
  "description": "Frontend for Sharinghood",
  "version": "0.1.0",
  "repository": "https://github.com/kevinbogao/sharinghood/tree/master/client",
  "contributors": [
    "Sparsh Tyagi <sparsh.tyagi@code.berlin>",
    "Bo Gao <bo.gao@code.berlin>"
  ],
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "graphql"
    ],
    "rules": {
      "indent": "off",
      "no-console": "off",
      "no-plusplus": "off",
      "no-underscore-dangle": "off",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ]
    }
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "babel": {
    "plugins": [
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "@styled-jsx/plugin-sass"
          ]
        }
      ],
      [
        "@babel/plugin-transform-typescript",
        {
          "allowNamespaces": true
        }
      ]
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@babel/runtime": "^7.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.31",
    "@fortawesome/free-solid-svg-icons": "^5.15.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "apollo-link-token-refresh": "^0.3.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "firebase": "^8.5.0",
    "graphql": "^15.5.0",
    "jspdf": "^2.1.1",
    "jwt-decode": "^3.0.0",
    "moment": "^2.29.0",
    "node-sass": "^6.0.0",
    "react": "^17.0.1",
    "react-dates": "^21.8.0",
    "react-dom": "^17.0.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-with-direction": "^1.3.1",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@styled-jsx/plugin-sass": "^3.0.0",
    "@types/react-dates": "^21.8.2",
    "@types/react-modal": "^3.12.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-jsx": "^2.2.8",
    "customize-cra": "^1.0.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "styled-jsx": "^3.3.0"
  }
}
